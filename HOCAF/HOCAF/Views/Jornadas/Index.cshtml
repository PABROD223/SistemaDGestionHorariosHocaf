@model IEnumerable<HOCAF.Models.Jornada>

@{
    ViewBag.Title = "Jotnada";
}

<h2>Jornada</h2>

<p>
    @Html.ActionLink("Crear Jornada", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.estado)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.estado)
        </td>
       
        <td>
            <a href="@Url.Action("Edit", new { id=item.idJornada  })"><span class="glyphicon glyphicon-edit"></span></a>
        </td>
        <td>
            <a href="@Url.Action("Details", new {id=item.idJornada })" class="text-success"><span class="glyphicon glyphicon-eye-open"></span></a>
        </td>
        <td>
            <a href="@Url.Action("Delete", new { id = item.idJornada })" class="text-danger"><span class="glyphicon glyphicon-remove"></span></a>
        </td>
    </tr>
}

</table>
